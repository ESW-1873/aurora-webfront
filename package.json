{
  "name": "aurora-webfront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:once": "jest",
    "storybook": "start-storybook  -s ./public,./src,.storybook -p 6006",
    "vrt": "yarn run-storycap && reg-suit run",
    "run-storycap": "cross-env NODE_ENV=vrt storycap http://localhost:6006 --serverCmd \"yarn storybook\" --serverTimeout 60000 -V 1920x1080 -V 390x844",
    "analyze:bundle": "cross-env ANALYZE=true next build",
    "generate-types:contract": "typechain --target ethers-v5 --out-dir src/external/contract/types 'aurora-core/abi/contracts/**/*.json'"
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^11.0.1",
    "@squardinc/next-seo": "^1.0.0",
    "@walletconnect/web3-provider": "^1.4.1",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "ethers": "^5.4.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "17.0.2",
    "react-media-hook": "^0.4.9",
    "react-tsparticles": "^1.32.0",
    "recoil": "^0.3.1",
    "styled-components": "^5.3.0",
    "web3": "^1.4.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/bundle-analyzer": "^11.0.1",
    "@storybook/addon-viewport": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@svgr/webpack": "^5.5.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/jest": "^26.0.24",
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "@types/styled-components": "^5.1.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "jest": "^27.0.6",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.3.2",
    "reg-keygen-git-hash-plugin": "^0.10.16",
    "reg-notify-github-plugin": "^0.10.16",
    "reg-notify-slack-plugin": "^0.10.16",
    "reg-publish-s3-plugin": "^0.10.16",
    "reg-suit": "^0.10.16",
    "storycap": "^3.0.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.2.0",
    "typechain": "^5.1.1",
    "typescript": "^4.3.5"
  }
}
