{
  "name": "hologram-webfront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:once": "jest",
    "storybook": "start-storybook  -s ./public,./src -p 6006",
    "vrt": "yarn run-storycap && reg-suit run",
    "run-storycap": "cross-env NODE_ENV=vrt storycap http://localhost:6006 --serverCmd \"yarn storybook\" --serverTimeout 60000 -V 1920x1080 -V 390x844"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.6",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-tsparticles": "^1.32.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@storybook/addon-viewport": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@svgr/webpack": "^5.5.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "@types/styled-components": "^5.1.11",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "reg-keygen-git-hash-plugin": "^0.10.16",
    "reg-notify-github-plugin": "^0.10.16",
    "reg-notify-slack-plugin": "^0.10.16",
    "reg-publish-s3-plugin": "^0.10.16",
    "reg-suit": "^0.10.16",
    "storycap": "^3.0.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
